#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/bin/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules/@istanbuljs/nyc-config-typescript/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules/@istanbuljs/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/bin/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/nyc@15.1.0/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules/@istanbuljs/nyc-config-typescript/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules/@istanbuljs/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/@istanbuljs+nyc-config-typescript@1.0.2_nyc@15.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nyc@15.1.0/node_modules/nyc/bin/nyc.js" "$@"
else
  exec node  "$basedir/../../../../../../nyc@15.1.0/node_modules/nyc/bin/nyc.js" "$@"
fi
