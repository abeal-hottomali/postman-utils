#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules/ts-node/dist/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules/ts-node/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules/ts-node/dist/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules/ts-node/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_cc3e19882e22ef587772a3bb1d93a70f/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../dist/bin-transpile.js" "$@"
fi
